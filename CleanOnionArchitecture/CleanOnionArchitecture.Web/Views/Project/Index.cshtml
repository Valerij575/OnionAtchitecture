@model ProjectViewModel

<table id="project" class="table table-bordered table-hover">
    <tr>
        <th>Id</th>
        <th>Name</th>
    </tr>
    <tbody>
        @foreach(var project in Model.Projects)
            {
                <tr>
                    <td>@project.Id</td>
                    <td>@project.Name</td>
                </tr>
            }
    </tbody>
</table>